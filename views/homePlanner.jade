
extends layout

block content
    div.navbar.navbar-fixed-top
        div.navbar-inner
            div.container-fluid
                ul#global-nav.nav.pull-left
                    a(href='#').brand Control Panel
                button#btn-logout.btn.btn-primary
                    i.icon-lock.icon-white
                    | Sign Out
            div.nav.nav-pills
                li.active(id='settings')
                    a
                        #account-settins Account settings
                li(id='TimeTable')
                    a
                        #addTimeTable addTimeTable

    include timetable
    include account
    include modals/alert
    include modals/form-errors
    include modals/confirm

block scripts
    script(src='/vendor/jquery.min.js')
    script(src='/vendor/jquery.form.js')
    script(src='/vendor/bootstrap-modal.js')
    script(src='/vendor/bootstrap-transition.js')
    script(src='/js/views/homePlanner.js')
    script(src='/js/controllers/homePlannerController.js')
    script(src='/js/form-validators/accountValidator.js')
    script(src='/js/form-validators/timetableValidator.js')
    script(src='/js/fullcalendar/lib/jquery-ui.custom.min.js')
    script(src='/js/fullcalendar/fullcalendar.js')
